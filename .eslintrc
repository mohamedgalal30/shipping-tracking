{
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": 'module',
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "no-undef": 0
  },
  "globals": {
    "module": true,
    "require": true,
    "sails": true,
    "exports": true
  }
}
